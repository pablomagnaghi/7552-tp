<?xml version="1.0" encoding="UTF-8"?>
<proyecto xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:noNamespaceSchemaLocation="composicion.xsd"
	nombre="ejemplo">
	<diagrama_principal nombre="principal">
		
		<entidad nombre="SeccionSala">
			<atributo nombre="nroSecc" tipo="integer"/>
			<atributo nombre="nomSecc" tipo="string"/>
			<identificador>
				<atributo>nroSecc</atributo>
			</identificador>
			<identificador>
				<atributo>nomSecc</atributo>
			</identificador>
			<relacion>SL</relacion>
		</entidad>
		
		<entidad nombre="Localidad">
			<atributo nombre="fila" tipo="integer" />
			<atributo nombre="nroButaca" tipo="integer" />
			<identificador>
				<atributo>fila</atributo>
				<atributo>nroButaca</atributo>
				<relacion>SL</relacion>
			</identificador>
			<relacion>SL</relacion>
		</entidad>
		
		<relacion>
			<entidad>SeccionSala</entidad> <!-- TODO definir un tipo de dato para que la cardinalidad quede junto a la entidad -->
			<entidad>Localidad</entidad>
			<cardinalidad>(1,n)</cardinalidad>
			<cardinalidad>(1,1)</cardinalidad>
		</relacion>
		
	</diagrama_principal>
</proyecto>

