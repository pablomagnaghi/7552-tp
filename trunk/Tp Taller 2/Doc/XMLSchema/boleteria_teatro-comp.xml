<?xml version="1.0" encoding="UTF-8"?>
<diagrama xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:noNamespaceSchemaLocation="composicion.xsd"
	nombre="boleteria teatro">
		
	<entidad codigo="1" nombre="SeccionSala" >
		<atributo codigo="2" nombre="nroSecc" tipo="caracterizacion" composicion="simple"/>
		<atributo codigo="3" nombre="nomSecc" tipo="caracterizacion" composicion="simple"/>
		<identificador>
			<atributo>nroSecc</atributo>
		</identificador>
		<identificador>
			<atributo>nomSecc</atributo>
		</identificador>
		<relacion>SL</relacion>
	</entidad>
	
	<entidad codigo="4" nombre="Localidad">
		<atributo codigo="5" nombre="fila" tipo="caracterizacion" composicion="simple" />
		<atributo codigo="6" nombre="nroButaca" tipo="caracterizacion" composicion="simple" />
		<identificador>
			<atributo>fila</atributo>
			<atributo>nroButaca</atributo>
			<relacion>SL</relacion>
		</identificador>
		<relacion>SL</relacion>
		<relacion>VtaLoc</relacion>
	</entidad>
	
	<entidad codigo="7" nombre="Obra">
		<atributo codigo="8" nombre="idObra" tipo="caracterizacion" composicion="simple" />
		<atributo codigo="9" nombre="titulo" tipo="caracterizacion" composicion="simple" />
		<atributo codigo="10" nombre="autor" tipo="caracterizacion" composicion="simple" cardinalidad="(1,n)" />
		<atributo codigo="11" nombre="director" tipo="caracterizacion" composicion="simple" cardinalidad="(1,n)" />
		<atributo codigo="12" nombre="genero" tipo="caracterizacion" composicion="simple" />
		<atributo codigo="13" nombre="nroProxFunc" tipo="caracterizacion" composicion="simple" />
		<identificador>
			<atributo>idObra</atributo>
		</identificador>
		<identificador>
			<atributo>titulo</atributo>
		</identificador>  
		<relacion>VtaLoc</relacion>
		<relacion>FO</relacion>
	</entidad>
	
	<entidad codigo="14" nombre="Funcion">
		<atributo codigo="15" nombre="nroFunc" tipo="caracterizacion" composicion="simple" />
		<atributo codigo="16" nombre="fecha" tipo="caracterizacion" composicion="simple"/>
		<atributo codigo="17" nombre="hora" tipo="caracterizacion" composicion="simple"/>
		<identificador>
			<atributo>nroFunc</atributo>
			<relacion>FO</relacion>
		</identificador>
		<identificador>
			<atributo>fecha</atributo>
			<atributo>hora</atributo>
			<relacion>FO</relacion>
		</identificador>
		<relacion>FO</relacion> 
	</entidad>
	
	<relacion codigo="18" nombre="SL">
		<entidad nombre="SeccionSala" cardinalidad="(1,n)" />
		<entidad nombre="Localidad" cardinalidad="(1,1)" />
	</relacion>
	
	<relacion codigo="21" nombre="VtaLoc">
		<entidad nombre="Localidad" cardinalidad="(0,n)" />
		<entidad nombre="Obra" cardinalidad="(0,n)" />
		<atributo codigo="22" nombre="tarj" tipo="caracterizacion" composicion="compuesto" cardinalidad="(0,1)">
			<atributo_hijo codigo="23" nombre="nomTarj" tipo="caracterizacion" composicion="simple" />
			<atributo_hijo codigo="24" nombre="bco" tipo="caracterizacion" composicion="simple" />
			<atributo_hijo codigo="25" nombre="nroTarj" tipo="caracterizacion" composicion="simple" />
			<atributo_hijo codigo="26" nombre="nroAutoriz" tipo="caracterizacion" composicion="simple" />
			<atributo_hijo codigo="27" nombre="cr" tipo="caracterizacion" composicion="compuesto" cardinalidad="(0,1)">
				<atributo_hijo codigo="28" nombre="cuotas" tipo="caracterizacion" composicion="simple" />
				<atributo_hijo codigo="29" nombre="int" tipo="caracterizacion" cardinalidad="(0,1)" composicion="simple" />
			</atributo_hijo>
		</atributo>
	</relacion>
	
	<relacion codigo="30" nombre="FO">
		<entidad nombre="Obra" cardinalidad="(0,n)"/>
		<entidad nombre="Funcion" cardinalidad="(1,1)" />
	</relacion>
	
</diagrama>

